@{
  Layout = "_Layout";
}

@using HairSalon.Models;
@model List<HairSalon.Models.Client>;

<h1>Stylists</h1>
@if (@Model.Count == 0)
{
  <h3>No stylists have been added yet!</h3>
}

@foreach (Client client in Model)
{
  @Html.ActionLink($"{client.Name}", "Details", new {id = client.ClientId})
}
@Html.ActionLink("Add new Stylists", "Create")